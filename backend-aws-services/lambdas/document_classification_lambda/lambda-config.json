{
    "FunctionName": "nmm_document_classification_lambda",
    "Runtime": "python3.9",
    "Role": "arn:aws:iam::{{ACCOUNT_ID}}:role/{{IAM_ROLE_NAME}}",
    "Handler": "lambda_function.lambda_handler",
    "Description": "Document classification for New Mexico Mutual worker compensation documents",
    "Timeout": 300,
    "MemorySize": 512,
    "TracingConfig": {
        "Mode": "PassThrough"
    },
    "PackageType": "Zip",
    "Architectures": [
        "x86_64"
    ],
    "EphemeralStorage": {
        "Size": 512
    },
    "Environment": {
        "Variables": {
            "EXTRACTION_TABLE": "{{EXTRACTION_TABLE_NAME}}",
            "DASHBOARD_TABLE": "{{DASHBOARD_TABLE_NAME}}",
            "ENTITY_EXTRACTION_LAMBDA": "{{ENTITY_EXTRACTION_LAMBDA_NAME}}"
        }
    }
}
