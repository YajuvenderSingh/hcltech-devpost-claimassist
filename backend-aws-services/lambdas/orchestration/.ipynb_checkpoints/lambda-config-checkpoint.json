{
    "FunctionName": "nmm-orchestration-lambda",
    "Runtime": "python3.12",
    "Role": "arn:aws:iam::{{ACCOUNT_ID}}:role/{{IAM_ROLE_NAME}}",
    "Handler": "lambda_function.lambda_handler",
    "Description": "Orchestration Lambda for claim processing workflow",
    "Timeout": 180,
    "MemorySize": 4096,
    "TracingConfig": {
        "Mode": "PassThrough"
    },
    "PackageType": "Zip",
    "Architectures": [
        "x86_64"
    ],
    "EphemeralStorage": {
        "Size": 4096
    },
    "Environment": {
        "Variables": {
            "DYNAMODB_TABLE_NAME": "{{DYNAMODB_TABLE_NAME}}",
            "SQS_QUEUE_URL": "{{SQS_QUEUE_URL}}",
            "PS_SQS_QUEUE_URL": "{{PS_SQS_QUEUE_URL}}"
        }
    }
}
